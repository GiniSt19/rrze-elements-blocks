{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rrze-elements/columns",
  "version": "0.0.6",
  "title": "Columns",
  "category": "design",
  "description": "Creates a columns block.",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Columns"
    },
    "numberOfColumns": {
      "type": "number",
      "default": 2
    },
    "rule": {
      "type": "boolean",
      "default": true
    },
    "width": {
      "type": "number",
      "default": 240
    },
    "borderColor": {
      "type": "string",
      "default": "#C3C3CB"
    }
  },
  "textdomain": "rrze-elements-b",
  "editorScript": "file:./index.ts",
  "script": "rrze-tabs",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}