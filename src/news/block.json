{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rrze-elements/news",
  "version": "0.0.6",
  "title": "News",
  "category": "design",
  "description": "Creates a news block.",
  "supports": {
    "html": false
  },
  "attributes": {
    "title" : {
      "type": "string"
    },
    "cat" : {
      "type": "string"
    },
    "tag" : {
      "type": "string"
    },
    "num" : {
      "type": "number",
      "default": 5
    },
    "divclass" : {
      "type": "string"
    },
    "hidemeta" : {
      "type": "boolean",
      "default": false
    },
    "hstart" : {
      "type": "number",
      "default": 2
    },
    "type" : {
      "type": "string"
    },
    "hideduplicates" : {
      "type": "boolean"
    },
    "has_thumbnail" : {
      "type": "boolean"
    },
    "days" : {
      "type": "number",
      "default": 0
    },
    "id" : {
      "type": "array"
    },
    "display" : {
      "type": "string",
      "default": ""
    },
    "hide" : {
      "type": "string"
    },
    "imgfloat" : {
      "type": "string",
      "default": "left"
    },
    "columns" : {
      "type": "number",
      "default": 0
    },
    "sticky_only" : {
      "type": "boolean"
    },
    "grid" : {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "rrze-elements-b",
  "editorScript": "file:./index.ts",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}